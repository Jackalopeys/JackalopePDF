<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\jackalope.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\jackalope.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PDFsharp" Version="6.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="JackalopePDF">
      <HintPath>lib\JackalopePDF.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
